

events
    _id       String
    owner     String
    created   String
    updated   String
    title     String
    location  String
    info      String
    bteam     Boolean
    private   Boolean
    shareId   String

venues
    _id       String
    title      String
    type      String
    tees      String
    pars[]    Number[]
    frontpar  Number
    backpar   Number
    yds[]     Number[]
    frontyds  Number
    backyds   Number

games
    _id       String
    title      String
  //  schedule  Date
    venue.id  String
    type      String
    event.id  String
    order     Number
    gamepts   Number[3]
    rules     String
    scores[
        {  playerId
        playerName
        raw
        finish
        }]

teams
    _id       String
    name      String
    games.id[] String[]

players
    _id       String
    name      String
    teamId    String
    games.id[] String[]

scores
    _id       String
    raw       Number
    finish    Number
    playerId  String
    teamId    String
    gameId    String
    eventId   String
